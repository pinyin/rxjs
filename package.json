{
  "name": "@pinyin/rxjs",
  "version": "0.0.6",
  "description": "rxjs helpers",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "sideEffects": false,
  "scripts": {
    "prepublishOnly": "rm -rf dist && npx tsc",
    "test": "tsc && npx jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pinyin/rxjs.git"
  },
  "keywords": [
    "rxjs"
  ],
  "author": "Bo Bao <bob432091@gmail.com> (https://github.com/pinyin)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pinyin/rxjs/issues"
  },
  "homepage": "https://github.com/pinyin/rxjs#readme",
  "devDependencies": {
    "@types/jest": "^23.1.2",
    "babel-polyfill": "^6.26.0",
    "jest": "^23.2.0",
    "rxjs": "^6.2.1",
    "rxjs-marbles": "^4.3.0",
    "ts-jest": "^22.4.6",
    "typescript": "^2.9.2"
  },
  "peerDependencies": {
    "rxjs": "^6.1.0"
  },
  "dependencies": {
    "@pinyin/maybe": "^0.0.7",
    "@pinyin/types": "^0.0.30",
    "tslib": "^1.9.3"
  },
  "jest": {
    "roots": [
      "src"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
